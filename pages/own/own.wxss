.own {
  background-color: #fcfcfa;
  padding-top: 200rpx;
}

.login-area {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  height: 200rpx;
}

.login {
  padding: 8px 10px;
  font-size: 30rpx;
  color: #3eda3e;
  border: 1px solid #3eda3e;
  border-radius: 10rpx;
}

.login-hover {
  background-color: #3eda3e;
  color: #fff;

  /* color: #ffffffc1; */
}


.login-txt {
  margin-top: 20rpx;
  color: gray;
  font-size: 25rpx;
}

.userinfo {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  margin-left: 30rpx;
}

.share {
  position: absolute;
  top: 0;
  right: 40rpx;
  width: 60rpx !important;
  height: 60rpx;
  padding: 0 !important;
  border-radius: 50%;
  /* background-color: red; */
  background-size: 40rpx 40rpx;
  background-position: center center;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}

.headphoto {
  position: relative;
  width: 150rpx;
  height: 150rpx;
}

.avatar {
  width: 100%;
  height: 100%;
  font-size: 0;
  border-radius: 50%;
  box-shadow: 0 0 15rpx 1rpx #eeeeee;
}

.info {
  margin-left: 30rpx;
  margin-top: 20rpx;
}

.nickname {
  font-size: 19px;
  max-width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.num {
  margin-top: 10rpx;
  color: #858585;
}

.btn-area {
  display: flex;
  margin: 30rpx 30rpx 0;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  color: #5b5b5b;
}

.new, .manage {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  flex-grow: 1;
  height: 100rpx;
  overflow: hidden;
  border-radius: 10rpx;
  box-shadow: 0 0 15rpx 1rpx #eeeeee;
}

.new {
  background-color: #d6ed42;
}

.new:before, .manage:before {
  content: '';
  display: block;
  background-size: cover;
  margin-right: 10rpx;
}

.new:before {
  width: 35rpx;
  height: 35rpx;
  background-image: url('data:image/png;base64,');
}

.manage:before {
  width: 33rpx;
  height: 33rpx;
  background-image: url('data:image/png;base64,');
}

.manage {
  margin-left: 20rpx;
  background-color: #59d0fc;
}

.btn-hover {
  opacity: .6;
}

.menu-list {
  margin-top: 50rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}

.menu-item {
  height: 90rpx;
  line-height: 90rpx;
  padding-left: 50rpx;
  border-bottom: 1px solid #f4efef;
}

.menu-item:last-child {
  border: none;
}

.menu-hover {
  background-color: #f7f4f4;
}

